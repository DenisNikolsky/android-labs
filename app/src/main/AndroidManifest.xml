<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.labs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Lab1Activity"
            android:label="@string/Lab1" />
        <activity
            android:name=".Lab2Activity"
            android:label="@string/Lab2" />
        <activity
            android:name=".Lab3Activity"
            android:label="@string/Lab3" />
        <activity
            android:name=".Lab4Activity"
            android:label="@string/Lab4" />
    </application>

</manifest>